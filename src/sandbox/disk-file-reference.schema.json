{
  "id": "http://workingspec.com/schema/disk-file-reference",
  "title": "Disk File Reference",
  "description": "Links to a Disk File",
  "type": "object",
  "properties": {
    "entityId": {
      "title": "Disk File",
      "type": "string",
      "pattern": "^[0-9a-f]{24}$",
      "message": "Disk File must be a 24 character hex string. (0-9, a-f)"
    },
    "entityType": {
      "title": "Entity Type",
      "type": "string",
      "enum": [
        "Disk File"
      ],
      "readOnly": true,
      "default": "Disk File"
    }
  },
  "required": [
    "entityId",
    "entityType"
  ],
  "additionalProperties": false
}