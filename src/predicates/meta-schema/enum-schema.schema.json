{
  "id": "http://workingspec.com/meta-schema/enum-schema",
  "title": "Enum Schema",
  "description": "A schema representing an enum",
  "type": "object",
  "properties": {
    "title": {
      "title": "Schema Title",
      "type": "string",
      "minLength": 1
    },
    "type": {
      "title": "Schema Type",
      "type": "string",
      "enum": [ "string" ]
    },
    "enum": {
      "title": "Enum Values",
      "description": "Should have same number of entries as `_esTitles`",
      "type": "array",
      "items": {
        "title": "Enum Value",
        "type": "string"
      }
    },
    "_esTitles": {
      "title": "Enum Titles",
      "description": "Should have same number of entries as `enum`",
      "type": "array",
      "items": {
        "title": "Enum Title",
        "type": "string"
      }
    }
  },
  "required": [ "title", "type", "enum", "_esTitles" ]
}