{
  "id": "http://workingspec.com/meta-schema/child-entity-schema",
  "title": "Child Entity Schema",
  "description": "An Entity Schema with a link to a Parent Entity",
  "type": "object",
  "properties": {
    "id": {
      "title": "Schema ID",
      "type": "string",
      "format": "uri"
    },
    "title": {
      "title": "Schema Title",
      "type": "string",
      "minLength": 1
    },
    "type": {
      "title": "Schema Type",
      "type": "string",
      "enum": [ "object" ]
    },
    "additionalProperties": {
      "title": "Additional Properties",
      "type": "boolean",
      "enum": [ false ],
      "default": "false"
    },
    "properties": {
      "title": "Properties",
      "type": "object",
      "additionalProperties": {
        "$ref": "http://workingspec.com/meta-schema/subschema"
      }
    },
    "_esParentKey": {
      "title": "Parent Property",
      "description": "Name of Property Pointing to Parent",
      "type": "string",
      "minLength": 1
    }
  },
  "required": [ "id", "title", "type", "additionalProperties", "properties", "_esParentKey" ]
}
